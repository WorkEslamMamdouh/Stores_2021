@using Inv.WebUI.Resources
@{
    Layout = "~/Views/Shared/Layout.cshtml";
}
<style>
    .st_border {
        border: solid 1px #3c8dbc;
        border-radius: 20px;
    }

    .display_none {
        display: none;
    }

    #divMeals {
        padding-left: 0px;
        padding-right: 0px;
    }

    #btnMeals:hover i {
        color: #3c8dbc !important;
        font-size: 25px;
    }

    #btnOtherservices:hover i {
        color: #3c8dbc !important;
        font-size: 25px;
    }

    #btnSubjects:hover i {
        color: #3c8dbc !important;
        font-size: 25px;
    }
</style>
<script>
    $(document).ready(function () { 
        $("#txt_ID_Vendor").select2();
     
        // Read selected option
        $('#but_read').click(function () {
            var username = $('#txt_ID_Vendor option:selected').text();
            var userid = $('#txt_ID_Vendor').val();
             

            $('#result').html("id : " + userid + ", name : " + username);
        });
    });
</script>
<label for="rpTitle" style="display:none" id="rpTitle">@*@SystemResource.Rep_PayRecieptList*@</label>
<div id="content_report" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12  margingred  borderred ">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
        <div id="informtionReserved" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 margingred" style="margin-bottom: 15px;">
            <div id="id_div_Add">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                    <label for="txtFromDate" class="col-lg-1 pading_2Supplieraccsta"> @SystemResource.App_FromDate </label>
                    <div class="col-xs-3 ">
                        <input type="date" class="input-sm form-control input-sm text-center " name="txtFromDate" id="txtFromDate" style="height: 33px;">
                    </div>
                    <label for="txtToDate" class="col-md-1 pading_2Supplieraccsta">  @SystemResource.App_ToDate  </label>
                    <div class="col-xs-3 ">
                        <input type="date" class="input-sm  form-control input-sm text-center" name="txtToDate" id="txtToDate" style="height: 33px;">
                    </div>
                    <div class="col-xs-4 ">
                        <label for="" class="col-xs-3 pading_2Supplieraccsta2"> @SystemResource.App_report_type</label>

                        <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4" style="color:#1cb3f9;font-weight:bold;">
                            <input type="radio" class="col-xs-3 form-control input-sm " id="Rd_detail" name="details" value="1">
                            <label for="" class="col-xs-8 pading_2  ">@SystemResource.Detailed  </label>
                        </div>

                        <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4" style="color:green;font-weight:bold;">
                            <input class="col-xs-3 form-control input-sm " type="radio" id="Rd_sum" name="details" value="2">
                            <label for="" class="col-xs-8 pading_2  ">@SystemResource.Summary  </label>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_2Supplieraccsta "> @SystemResource.APP_Category  </label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3">
                        <select id="txt_ID_APP_Category" name="" class="form-control input-sm  text_Display">
                            <option value="Null"> @SystemResource.choose_Cat </option>
                        </select>
                    </div>
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_2Supplieraccsta "> @SystemResource.App_Group</label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3">
                        <select id="txt_ID_APP_Group" name="" class="form-control input-sm  text_Display">
                            <option value="Null">@SystemResource.Choose_group</option>
                        </select>
                    </div>
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_2Supplieraccsta ">@SystemResource.App_Status</label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3">
                        <select id="txt_ID_APP_Type" name="" class="form-control input-sm  text_Display">
                            <option value="Null">@SystemResource.App_All</option>
                            <option value="1">@SystemResource.prepared</option>
                            @*<option value="0">@SystemResource.unprepared</option>*@
                        </select>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_2Supplieraccsta ">@SystemResource.I_Vendor</label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3">
                        <select id="txt_ID_Vendor" name="" class="form-control input-sm  text_Display">
                            <option value="Null">@SystemResource.App_Everybody</option>
                        </select>
                    </div>
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_2Supplieraccsta ">@SystemResource.App_indebtedness</label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3">
                        <select id="txt_indebtedness" name="" class="form-control input-sm  text_Display">
                            <option value="All">@SystemResource.App_All</option>
                            <option value=">">@SystemResource.App_He_Owes_Him </option>
                            <option value="<">@SystemResource.App_an_indebtedness</option>
                            <option value="=">@SystemResource.App_Zero </option>
                        </select>
                    </div><label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_2Supplieraccsta ">@SystemResource.activity_Nature </label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3">
                        <select id="txtVendorType" name="" class="form-control input-sm  text_Display">
                            <option value="Null">@SystemResource.App_Everybody</option>
                            <option value="1">@SystemResource.vendor_good</option>
                            <option value="2"> @SystemResource.Service_supplier</option>
                        </select>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id=" " class=" col-md-12 col-sm-12 col-xl-12 col-xs-12 margingred">
    <div class=" col-md-12 col-sm-12 col-xl-12 col-xs-12">
        <div class="col-lg-1 hidden-md hidden-sm hidden-xs ">  </div>
        <div class="col-lg-10 col-md-10 col-sm-10 col-xl-10 col-xs-10 borderred" style="right: 3%;">
            <div class="col-lg-1 hidden-md hidden-sm hidden-xs "></div>
            <button id="btnReset" type="button" class="marginbtn btn btn-custon-four btndefolt primary-btn-cl  ">  @SystemResource.new_search <span class="glyphicon glyphicon-search "></span> </button>
            <button id="btnviewresult" type="button" class="  btn btn-custon-four btndefolt primary-btn-cl  " style="margin-right: 1%;display:none"><span class="adminpro-icon adminpro-paint-roller"></span>   @SystemResource.Preview_results  </button>
            <div class="col-lg-1 hidden-md hidden-sm hidden-xs "></div>
            <button id="btnPrintTrview" type="button" class="marginbtn btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-airplay"></span>@SystemResource.Print_Report</button>
            <button id="btnPrintTrPDF" type="button" class="marginbtn btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-download-arrow"></span>@SystemResource.Download_PDF</button>
            <button id="btnPrintTrEXEL" type="button" class="marginbtn btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-download-arrow"></span>@SystemResource.Download_EXCEL</button>
            <button id="btnPrint" type="button" class="  btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-paint-roller"></span> @SystemResource.App_Print</button>
        </div>
    </div>
</div>
